<style>
	.ztree{
		overflow: auto;
	}
</style>
<script>

	$(function(){
		countModelBarByMonth();
		countModelBarByYestday();
		// 路径配置
		require.config({
			paths: {
				echarts : '${ctxPath}/static/echarts-2.1.10/build/dist'
			}
		});
		countByPie(1);
	})

	//饼形图
	function countByPie(flag){
		var data; 
		var models = [];
		var title;
		$.ajax({
			url: "${ctxPath}/risk/detection/countByPie?flag="+flag,
			method:"post",
			dataType: "json",
			async: false,
			success: function(result) {
				data = result
				$.each(result,function(i,obj){
					models.push(obj.name);
				})
			}
		}); 
		if(flag == 1){
			title = '上月模型跑批饼图';
		}else{
			title = '昨日模型跑批饼图';
		}
		var option = {
			title : {
				text: '',
				subtext: '',
				x:'center'
			},
			tooltip : {
				trigger: 'item',
				formatter: "{a} <br/>{b} : {c} ({d}%)"
			},
			legend: {
				orient : 'vertical',
				x : 'left',
				data : models
			},
			toolbox: {
				show : true,
				feature : {
					mark : {show: true},
					dataView : {show: true, readOnly: false},
					magicType : {
						show: true,
						type: ['pie', 'funnel'],
						option: {
							funnel: {
								x: '25%',
								width: '50%',
								funnelAlign: 'left',
								max: 1548
							}
						}
					},
					restore : {show: true},
					saveAsImage : {
				        show : true,
				        title : '保存为图片',
				        type : 'png',
				        lang : ['点击保存']
				    }
				}
			},
			calculable : true,
			
			series : [
				{
					name: title,
					type:'pie',
					radius : '55%',
					center: ['50%', '60%'],
					data: data
				}
			]
			
		};
		require(
				[
					'echarts',
					'echarts/chart/pie',
					'echarts/chart/funnel'
				],
				function (ec) {
					var myChart = ec.init(document.getElementById('picBody'));
					var ecConfig = require('echarts/config');
					myChart.setOption(option,true);
				}
		);
	}

	//昨日机构排行榜
	function countModelBarByYestday(){
		var ogranid = [];
		var count = [];
		$.ajax({
			url: "${ctxPath}/risk/detection/findByOrgan?flag=0",
			method:"post",
			dataType: "json",
			async: false,
			success: function(data) {
				$.each(data,function(i,obj){
					ogranid.push(obj.organid);
					count.push(obj.count);
				})
			}
		});
		var option = {
			title : {
				text: '',
				subtext: ''
			},
			tooltip : {
				trigger: 'axis'
			},
			toolbox: {
				show : true,
				feature : {
					mark : {show: true},
					dataView : {show: true, readOnly: false},
					magicType : {show: true, type: ['line', 'bar']},
					restore : {show: true},
					saveAsImage : {show: true}
				}
			},
			yAxis : [
				{
					type : 'value',
					boundaryGap : [0, 0.01]
				}
			],
			calculable : true,
			xAxis : [
				{
					type : 'category',
					data:ogranid
				}
			],
			series : [
				{
					type:'bar',
					data:count
				}
			]
		};

		require(
				[
					'echarts',
					'echarts/chart/line',
					'echarts/chart/bar'
				],
				function (ec) {
					var myChart = ec.init(document.getElementById('picBody2'));
					var ecConfig = require('echarts/config');
					myChart.setOption(option,true);
				}
		);
	}
	
	//上月机构排行榜
	function countModelBarByMonth(){
		var ogranid = [];
		var count = [];
		$.ajax({
			url: "${ctxPath}/risk/detection/findByOrgan?flag=1",
			method:"post",
			dataType: "json",
			async: false,
			success: function(data) {
				$.each(data,function(i,obj){
					ogranid.push(obj.organid);
					count.push(obj.count);
				})
			}
		});
		var option = {
			title : {
				text: '',
				subtext: ''
			},
			tooltip : {
				trigger: 'axis'
			},
			toolbox: {
				show : true,
				feature : {
					mark : {show: true},
					dataView : {show: true, readOnly: false},
					magicType : {show: true, type: ['line', 'bar']},
					restore : {show: true},
					saveAsImage : {show: true}
				}
			},
			yAxis : [
				{
					type : 'value',
					boundaryGap : [0, 0.01]
				}
			],
			calculable : true,
			xAxis : [
				{
					type : 'category',
					data:ogranid
				}
			],
			series : [
				{
					type:'bar',
					data:count
				}
			]
		};

		require(
				[
					'echarts',
					'echarts/chart/line',
					'echarts/chart/bar'
				],
				function (ec) {
					var myChart = ec.init(document.getElementById('picBody1'));
					var ecConfig = require('echarts/config');
					myChart.setOption(option,true);
				}
		);
	}

	//柱状图
	function countByBar(){
		var models = []; 
		var value1 = [];
		var value2 = [];
		$.ajax({
			url: "${ctxPath}/risk/detection/countByBar",
			method:"post",
			dataType: "json",
			async: false,
			success: function(result) {
				$.each(result,function(i,obj){
					models.push(obj.key);
					value1.push(obj.value1);
					value2.push(obj.value2);
				})
			}
		}); 
		
		var option = {
			title : {
				text: '',
				subtext: ''
			},
			tooltip : {
				trigger: 'axis'
			},
			legend: {
				data:['上月','昨日'],
				y:'bottom'
			},
			toolbox: {
				show : true,
				feature : {
					mark : {show: true},
					dataView : {show: true, readOnly: false},
					magicType : {show: true, type: ['line', 'bar']},
					restore : {show: true},
					saveAsImage : {show: true}
				}
			},
			xAxis : [
				{
					type : 'value',
					boundaryGap : [0, 0.01]
				}
			],
			calculable : true,
			yAxis : [
				{
					type : 'category',
					data : models
				}
			],

			series : [
				{
					name:'上月',
					type:'bar',
					data:value1
				},
				{
					name:'昨日',
					type:'bar',
					data:value2
				}
			]
		};

		require(
				[
					'echarts',
					'echarts/chart/line',
					'echarts/chart/bar'
				],
				function (ec) {
					var myChart = ec.init(document.getElementById('picBody'));
					var ecConfig = require('echarts/config');
					myChart.setOption(option,true);
				}
		);
	}

	function clickGoto(name){
		var a = document.getElementById(name);
		var link = a.getAttribute('href');
		if(name == "合规模型"){
			a.href = "#/ajax/risk/hg";
		}else if(name == "监管模型"){
			a.href = "#/ajax/risk/jg";
		}else if(name == "重大风险模型"){
			a.href = "#/ajax/risk/big";
		}else if(name == "审计专项模型"){
			a.href = "#/ajax/risk/detection/sjzx";
		}else if(name == "二次分析模型"){
			a.href = "#/ajax/second/model";
		}
	}
	
	function clickGoto_1(name){
		var a = document.getElementById(name+"_1");
		var link = a.getAttribute('href');
		if(name == "合规模型"){
			a.href = "#/ajax/risk/hg";
		}else if(name == "监管模型"){
			a.href = "#/ajax/risk/jg";
		}else if(name == "重大风险模型"){
			a.href = "#/ajax/risk/big";
		}else if(name == "审计专项模型"){
			a.href = "#/ajax/risk/detection/sjzx";
		}else if(name == "二次分析模型"){
			a.href = "#/ajax/second/model";
		}
	}
	
